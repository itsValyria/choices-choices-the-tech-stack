---
import Layout from '../layouts/layout.astro';
import { fetchData } from '../lib/fetch-api-data';

// Fetching data from the Directus API
const { webinars, contourings } = await fetchData();
---

<Layout title="Oncollaboration | Home" description="Where radiotherapists all across the world meet.">
  <h1>Hello, world!</h1>
  <p>Welcome to my website.</p>

  <h2>Webinars</h2>
  <ul>
    {webinars.map(webinar => (
      <li>{webinar.title}</li> // Removed key here
    ))}
  </ul>

  <h2>Contourings</h2>
  <ul>
    {contourings.map(contouring => (
      <li>{contouring.title}</li> // Removed key here
    ))}
  </ul>
</Layout>